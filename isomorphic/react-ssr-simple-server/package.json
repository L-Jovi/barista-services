    "express": "^4.14.1",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "start-server-webpack-plugin": "^2.1.2",
    "webpack": "^5.74.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-node-externals": "^1.5.4"
  }
}
